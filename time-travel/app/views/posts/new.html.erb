Create New Post

<%= form_for @post do |f| %>
  <div>
    <%= f.text_field :title, placeholder: "Title" %>
  </div>
  <div>
    <%= f.text_area :description, placeholder: "Description" %>
  </div>
  <div>
    <%= f.text_field :image, placeholder: "Image" %>
  </div>
  <div>
    <%= f.date_field :post_date, placeholder: "Date" %>
  </div>
  <%= f.submit "Create Post" %>
<% end %>

   
 